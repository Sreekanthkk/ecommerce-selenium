    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
         stage('UAT') {
            steps {
                echo 'uat'
            }
        }
         stage('Test') {
            steps {
                echo 'test'
            }
        }
         stage('PROD') {
            steps {
                echo 'prod'
            }
        }
    }
}
